<?xml version="1.0"?>
<object name="qiproject"
   meta_type="Factory-based Type Information with dynamic views"
   i18n:domain="Products.qi" xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title" i18n:translate="">QI Project</property>
 <property name="description"
    i18n:translate="">A quality improvement project workspace</property>
 <property name="content_icon">++resource++qiproject_icon.png</property>
 <property name="content_meta_type">qiproject</property>
 <property name="product"></property>
 <property name="factory">Products.qi.Project</property>
 <property name="immediate_view">@@edit</property>
 <property name="global_allow">True</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types">
  <element value="Discussion Item"/>
  <element value="Document"/>
  <element value="Event"/>
  <element value="File"/>
  <element value="Image"/>
  <element value="Link"/>
  <element value="News Item"/>
  <element value="Project_Folder"/>
  <element value="Topic"/>
  <element value="qichart"/>
  <element value="qiteam"/>
  <element value="qi.ChartHolder"/>
 </property>
 <property name="allow_discussion">False</property>
 <property name="default_view">folder_listing</property>
 <property name="view_methods">
  <element value="folder_summary_view"/>
  <element value="folder_tabular_view"/>
  <element value="folder_listing"/>
 </property>
 <property name="default_view_fallback">False</property>
 <alias from="(Default)" to="(dynamic view)"/>
 <alias from="edit" to="@@edit"/>
 <alias from="view" to="(selected layout)"/>
 <alias from="z3view" to="@@view"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    url_expr="string:${object_url}/" visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    url_expr="string:${object_url}/edit" visible="True">
  <permission value="Modify portal content"/>
 </action>
</object>
