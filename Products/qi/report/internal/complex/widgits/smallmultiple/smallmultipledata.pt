<?xml version="1.0" encoding="UTF-8"?>
<chart 
      xmlns:tal="http://xml.zope.org/namespaces/tal">

	<series>
	    <tal:dates repeat="date context/dates">
	        <value tal:attributes="xid date" tal:content="python:date.strftime('%m/%Y')" />
        </tal:dates>
        
	</series>
	<graphs>
		<graph gid="team">
		    <tal:data repeat="date context/dates">
	            <value tal:define="value python: context.getValue(date)"
	                 tal:attributes="xid date" description="example"
	                tal:content="value" tal:condition="python: value is not None" />
	        </tal:data>
		</graph>
	</graphs>
</chart>
