<configure
    xmlns="http://namespaces.zope.org/zope"
     xmlns:browser="http://namespaces.zope.org/browser"
     >
     <browser:page
        for=".interfaces.IGoogleGadget"
        name="dataurl"
        class=".googlequery.QueryData"
        permission="zope2.View"
        />
        
    <browser:page
        name="editblurb"
        for=".interfaces.IGoogleGadget"
        template="edit.pt"
        permission="cmf.ModifyPortalContent"
        />
</configure>