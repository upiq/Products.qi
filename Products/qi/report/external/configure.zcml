<configure 
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="Products.qi">
    

    <browser:page
        for="Products.qi.extranet.types.interfaces.IProject"
        name="reports.html"
        class=".reportPage.ReportPage"
        template="project_reports.pt"
        permission="zope2.View"
        />
	<browser:page
		for="Products.qi.extranet.types.interfaces.IProject"
		name="ReportBatches"
		class=".reportPage.Batches"
		template="batches.pt"
		permission="zope2.View"
		/>

    <browser:page
        for="*"
        name="addreport.html"
        class=".reportPage.AddReportForm"
        template="newreport.pt"
        permission="cmf.ModifyPortalContent"
        />
    <browser:page
        for="Products.qi.extranet.types.interfaces.IProject"
        name="approve"
        class=".reportPage.Batch"
        template="batch.pt"
        permission="cmf.ModifyPortalContent"
        />
    <browser:page
        for="Products.qi.extranet.types.interfaces.ITeam"
        name="reports.html"
        class=".reportPage.TeamReports"
        template="team_reports.pt"
        permission="zope2.View"
        />
    <browser:page
        for="Products.qi.extranet.types.interfaces.IProject"
        name="show_reports.html"
        class=".reportPage.ProjectReports"
        template="team_reports.pt"
        permission="zope2.View"
        />

    <browser:page
        for="*"
        name="updatestatus"
        class=".reportPage.UpdateRunningState"
        permission="zope2.View"
        />
        
    <browser:page
        for="*"
        name="startreport"
        class=".reportPage.StartReport"
        permission="zope2.View"
        />
    <browser:resource
        file="report.kss"
        name="report.kss"
        />
    
    <browser:page
        for="Products.qi.extranet.types.interfaces.IProject"
        name="expandbatches"
        class=".reportPage.ExpandBatches"
        template="batches.pt"
        permission="zope2.View"
        />

    <browser:page
        for="Products.qi.extranet.types.interfaces.IProject"
        name="collapsebatches"
        class=".reportPage.CollapseBatches"
        permission="zope2.View"
        />

     <!--   
    <browser:addMenuItem
        title="reportgroup ReportManager"
        factory="Products.qi.reports.ReportManager"
        view="Products.qi.reports.ReportManager"
        permission="Products.qi.reports.AddReport"
        />
        -->
</configure>
