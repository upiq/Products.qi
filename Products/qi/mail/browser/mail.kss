div.available:click
{
    action-server: subscribeto;
    subscribeto-listid: currentFormVar("listid");
}

div.joined:click
{
    action-server: unsubscribeto;
    unsubscribeto-listid: currentFormVar("listid");
}


div.subupdate:click
{
